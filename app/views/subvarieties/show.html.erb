<table  id="subvarieties">

  <h2><%= @subvariety.name %></h2>

  <ul>
    <li><%= link_to 'Edit Subvariety', edit_variety_subvariety_path(@variety, @subvariety)%></li>
    <li><%= link_to "Delete Subvariety", [@variety, @subvariety], method: :delete,
                    data: {confirm: 'Are you sure you want to delete this Subvariety?'} %></li>
  </ul>
</table>
